<div class="container mt-4">
  <div class="row">

    <div class="col-md-8 mb-5">
      <h4 class="jumbotron p-2 text-center">Timeline</h4>
      <ul class="timeline">
        {{#each events}}
        {{> project/event-block}}
        {{/each}}
      </ul>

    </div>

    <div class="col-md-4 mb-5">
      <h4 class="jumbotron p-2 text-center">Projects</h4>
      {{#each user.Projects}}
      {{> user/project-block}}
      {{/each}}
    </div>

  </div>
</div>